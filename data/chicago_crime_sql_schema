CREATE TABLE crime_data (
	ID varchar NOT NULL,
	Block varchar NOT NULL,
	IUCR int NOT NULL,
	Primary_Type varchar NOT NULL,
	Violence_Status varchar NOT NULL,
	Description varchar NOT NULL,
	Location_Description varchar NOT NULL,
	Arrest bool NOT NULL,
	Demestic bool NOT NULL,
	District int NOT NULL,
	Ward int NOT NULL,
	Community_Area int NOT NULL,
	FBI_Code varchar NOT NULL,
	Year date NOT NULL,
	Latitude float NOT NULL,
	Longitude float NOT NULL,
	Date date NOT NULL,
	Time date NOT NULL,
	PRIMARY KEY (Date),
	UNIQUE (ID)
);

CREATE TABLE weather_data(
	Station varchar NOT NULL,
	Latitude float NOT NULL,
	Longitude float NOT NULL,
	Elevation float NOT NULL,
	Date date  NOT NULL,
	Average_Wind_Speed float NOT NULL,
	Time_of_Fastest_Wind int NOT NULL,
	Peak_Gust_Time int NOT NULL,
	Average_Temperature int NOT NULL,
	Maximum_Temperature int NOT NULL,
	Minimum_Temperature int NOT NULL,
	Direction_Fastest_2_Minute_Wind int NOT NULL,
	Direction_Fastest_5_Minute_Wind int NOT NULL,
	Fastest_2_Minute_Wind_Speed float NOT NULL,
	Fastest_5_Minute_Wind_Speed float NOT NULL,
	Fog_Ice_Freezing_Fog int NOT NULL,
	Heavy_Fog_or_Heavy_Freezing_Fog int NOT NULL,
	Thunder int NOT NULL,
	Ice_Pellets_Sleet_or_Small_Hail int NOT NULL,
	Hail int NOT NULL,
	Glaze_or_Rime int NOT NULL,
	Dust_Volcanic_Ash_or_Blowing_Dust int NOT NULL,
	Smoke_or_Haze int NOT NULL,
	Blowing_or_Drifting_Snow int NOT NULL,
	High_or_Damaging_Winds int NOT NULL,
	Mist int NOT NULL,
	Drizzle int NOT NULL,
	Freezing_Drizzle int NOT NULL,
	Rain int NOT NULL,
	Freezing_Rain int NOT NULL,
	Snow_Snow_Pellets_or_Ice_Crystals int NOT NULL,
	Unknown_Precipitation int NOT NULL,
	Ground_Fog int NOT NULL,
	Ice_fog_or_Freezing_Fog int NOT NULL,
	PRIMARY KEY (Date),
	UNIQUE (Date)
);